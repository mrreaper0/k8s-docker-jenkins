pipeline{
  agent any

  environment{
    PATH = "/opt/apache-maven-3.6.3/bin:$PATH"
  }
  stages{
    stage("Git Checkout"){
      steps{
        git credentialsId: 'github', url: 'https://github.com/mrreaper0/k8s-docker-jenkins.git'
      }
    }
    stage("Maven Build){
      steps{
        sh "mvn clean package"
      }
    }
  }
}
